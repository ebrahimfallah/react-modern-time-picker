{
   "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "lib": ["DOM", "ESNext"],
      "jsx": "react",
      "declaration": true,
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "paths": {
         "@/*": ["./src/*"]
      }
   },
   "include": ["src"]
}